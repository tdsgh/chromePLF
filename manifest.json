{
  "manifest_version": 2,

  "name": "Profitero login forms",
  "description": "Fill login forms into Profitero apps",
  "version": "1.0",

  "background" : {
    "scripts": ["plf-background.js"],
    "persistent": false
  },

  "page_action": {
    "default_icon": "plf-icon.png",
    "default_popup": "plf-popup.html"
  },
  "permissions": [
    "declarativeContent"
  ]
}